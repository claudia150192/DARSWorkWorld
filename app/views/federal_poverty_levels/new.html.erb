<% title "New federal poverty level" %>

<% form_for(@federal_poverty_level) do |f| %>
  <% @federal_poverty_level.errors.full_messages.each do |msg| %>
      <p><%= msg %></p>
  <% end %>
	<%= render :partial => "form", :object => f %>
  <%= f.submit "Create", :class => "button" %> <%= link_to 'Cancel', :back %>
<% end %>
