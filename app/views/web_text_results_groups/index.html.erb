<% title "Listing web text results groups" %>

<table>
  <tr>
    <th>Name</th>
    <th>Order</th>
  </tr>

<% @web_text_results_groups.each do |web_text_results_group| %>
  <tr>
    <td><%=h web_text_results_group.name %></td>
    <td><%=h web_text_results_group.order %></td>
    <td><%= link_to 'Show', web_text_results_group %></td>
    <td><%= link_to 'Edit', edit_web_text_results_group_path(web_text_results_group) %></td>
    <td><%= link_to 'Delete', web_text_results_group, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New web text results group', new_web_text_results_group_path %>