<% title "Create Question Validation Message Insertion" %>

<% form_for(@question_validation_insert) do |f| %>
  <% (@question_validation_insert.errors.full_messages.each do |msg| %>
      <p><%= msg %></p>
  <% end %>
	<%= render :partial => "form", :object => f %>
  <p>
    <%= f.submit 'Create', :class => "button" %> <%= link_to 'Cancel', :back %>
  </p>
<% end %>
