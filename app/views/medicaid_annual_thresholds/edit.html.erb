<% title "Editing medicaid annual threshold" %>

<% form_for(@medicaid_annual_threshold) do |f| %>
  <% @medicaid_annual_threshold.errors.full_messages.each do |msg| %>
      <p><%= msg %></p>
  <% end %>
	<%= render :partial => "form", :object => f %>
  <%= f.submit "Update", :class => "button" %> <%= link_to 'Cancel', :back %>
<% end %>
