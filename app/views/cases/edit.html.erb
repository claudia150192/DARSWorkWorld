<h1>Edit Case Notes</h1>

<%= form_for(@case) do |f| %>
  <% @case.errors.full_messages.each do |msg| %>
      <p><%= msg %></p>
  <% end %>

  <%= render :partial => "form", :object => f %>
  <p>
    <%= f.submit 'Update', :class => "button" %> <%= link_to 'Cancel', :back %>
  </p>
<% end %>

<% if can? :manage, @question %>
	<%= link_to 'Back to cases', cases_path %>
<% end %>
